  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "start-dev": "env-cmd -e dev GENERATE_SOURCEMAP=true react-scripts start",
    "build-dev": "env-cmd -e dev GENERATE_SOURCEMAP=true react-scripts build",
    "start-qa": "env-cmd -e qa GENERATE_SOURCEMAP=false react-scripts start",
    "build-qa": "env-cmd -e qa GENERATE_SOURCEMAP=false react-scripts build",
    "start-prod": "env-cmd -e prod GENERATE_SOURCEMAP=false react-scripts start",
    "build-prod": "env-cmd -e prod GENERATE_SOURCEMAP=false react-scripts build",
    "start-development": "cross-env REACT_APP_ENV=development GENERATE_SOURCEMAP=true react-scripts start",
    "build-development": "cross-env REACT_APP_ENV=development GENERATE_SOURCEMAP=true react-scripts build",
    "start-test": "cross-env REACT_APP_ENV=test GENERATE_SOURCEMAP=false react-scripts start",
    "build-test": "cross-env REACT_APP_ENV=test GENERATE_SOURCEMAP=false react-scripts build",
    "start-production": "cross-env REACT_APP_ENV=production GENERATE_SOURCEMAP=false react-scripts start",
    "build-production": "cross-env REACT_APP_ENV=production GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie >=9"
    ]
  },
  "devDependencies": {
    "@types/create-react-class": "^15.6.2",

    "@types/react-datepicker": "^2.8.0",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "@types/yup": "^0.26.21",
    "cross-env": "^5.2.0",
    "env-cmd": "^9.0.3",
    "source-map-explorer": "^2.0.1",
    "terser-brunch": "^3.0.0"
  }
